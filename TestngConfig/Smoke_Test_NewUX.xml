<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="selenium-smoke-suite" parallel="classes"
       thread-count="1">
    <test name="smoke-tests">
        <parameter name="browserName" value="chrome"/>
        <parameter name="project" value="MyScript"/>
        <groups>
            <run>
                <include name="Smoke_Test_NewUX"/>
            </run>
        </groups>
        <classes>
            <class name="selenium.AdminThroughRegistration"/>
        </classes>
    </test>
</suite>
